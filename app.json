{
  "expo": {
    "name": "robopits-app",
    "slug": "robopits-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icono-app.png",
    "userInterfaceStyle": "light",
    "scheme": ["robopitsapp"],
    "splash": {
      "image": "./assets/logo-robopits.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icono-app.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.angel06.robopitsapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a61edb05-35b6-4f82-a7ee-ae7bf5213a9c"
      }
    }
  }
}
